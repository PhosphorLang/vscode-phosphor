{
    "name": "vscode-phosphor",
    "displayName": "Phosphor",
    "description": "Phosphor for Visual Studio Code",
    "author": "Benedikt Magnus",
    "publisher": "PhosphorLang",
    "version": "0.8.0",
    "license": "MIT",
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/PhosphorLang/vscode-phosphor"
    },
    "bugs": {
        "url": "https://github.com/PhosphorLang/vscode-phosphor/issues"
    },
    "engines": {
        "vscode": "^1.84.0"
    },
    "contributes": {
        "languages": [
            {
                "id": "phosphor",
                "aliases": ["Phosphor", "phosphor"],
                "extensions": [".ph",".phosphor"],
                "configuration": "./language-configuration.json",
                "icon": {
                    "dark": "./icons/icon.svg",
                    "light": "./icons/icon.svg"
                }
            }
        ],
        "grammars": [
            {
                "language": "phosphor",
                "scopeName": "source.ph",
                "path": "./syntaxes/phosphor.tmLanguage.json"
            }
        ]
    }
}
